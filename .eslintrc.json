{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-param-reassign": 0,
        "no-plusplus": 0,
        "no-use-before-define": ["error", { "variables": false, "functions": false}],
        "no-return-assign": [1, "except-parens"]
    }
}
